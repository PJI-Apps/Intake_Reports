# PJI Law Reports - Security .gitignore
# This file prevents sensitive data from being committed to Git

# Streamlit secrets and configuration
.streamlit/secrets.toml
.streamlit/config.toml

# Data files (real data should never be committed)
*.csv
*.xlsx
*.xls
*.xlsm
data/
uploads/
downloads/

# Test data (optional - you can commit test data if needed)
test_*.csv
test_*.xlsx
test_data/

# Logs and temporary files
*.log
logs/
temp/
tmp/
*.tmp

# Python cache and compiled files
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg

# Virtual environments
venv/
env/
ENV/
.venv/
.env/

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Backup files
*.bak
*.backup
*.old

# Google service account keys
*.json
service-account-*.json
credentials.json

# Environment variables
.env
.env.local
.env.production

# Database files
*.db
*.sqlite
*.sqlite3

# Jupyter notebooks (if any)
.ipynb_checkpoints/

# Coverage reports
htmlcov/
.coverage
.coverage.*
coverage.xml
*.cover

# Security notes
SECURITY.md
secrets.txt
passwords.txt

# Test outputs
test_outputs/
test_results/

# Temporary uploads
uploaded_files/
processed_files/

# Cache directories
.cache/
.streamlit/cache/

# Local development files
local_config.py
dev_settings.py
